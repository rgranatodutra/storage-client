generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model File {
  id   String   @id
  name String
  type String
  size Int
  path String
  date DateTime @default(now())

  @@map("files")
}
